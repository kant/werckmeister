using "./chords/default.chords";


device: SC1 midi 0;
--           instrument   dv ch cc pc
instrumentDef: piano      SC1 0 0 0;

[
instrument: piano;
type: template;
name: bg;
{
    I I I I |
}
]

[
instrument: piano;
type: template;
name: bg2;
{
    III,&&& |
}
]

[
instrument: piano;
type: template;
name: myfill;
{
    I'' &&& |
}
]

[
instrument: piano;
type: template;
name: myfill2;
{
    I,, &&& |
}
]

[
type: accomp;
{
    /template: bg/
    C | 
    /fill: _template=myfill _replace=bg/
    C | C | 
    /fill: _template=myfill/
    C | C |
    /fill: _template=myfill2 _replace=bg/
    C | C |
    /template: _template=bg2/
    C |
    --/fill: _template=myfill/
    /fill: _template=myfill2/
    C | C |
    /template: bg bg2/
    /fill: _template=myfill/
    --/fill: _template=myfill2/
    C | C |
    /template: bg/
    C | 
}
]
